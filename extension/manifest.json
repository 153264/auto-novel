{
    "manifest_version": 3,
    "name": "CRXJS Vue Vite Example",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "webRequest",
        "http://localhost:5173/"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "src/background.ts"
    },
    "content_scripts": [
        {
            "matches": [
                "https://books.fishhawk.top/",
                "http://localhost:5173/"
            ],
            "js": [
                "src/relay.ts"
            ]
        }
    ]
}